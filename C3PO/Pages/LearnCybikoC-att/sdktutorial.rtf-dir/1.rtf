\par {\listtext\pard\plain\s1 \b\f1\fs28\kerning28\cgrid \hich\af1\dbch\af0\loch\f1 Part I : \tab}}\pard\plain \s1\sb240\sa60\keepn\pagebb\nowidctlpar\widctlpar\jclisttab\tx1440\ls5\outlinelevel0\adjustright \b\f1\fs28\kerning28\cgrid {Getting Started
